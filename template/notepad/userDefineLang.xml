<NotepadPlus>
<!-- %SHILL_UPDATE_NOTICE% -->
    <UserLang name="LSL 1.23.4" ext="">
        <Settings>
            <Global caseIgnored="no" />
            <TreatAsSymbol comment="no" commentLine="no" />
            <Prefix words1="no" words2="no" words3="no" words4="no" />
        </Settings>
        <KeywordLists>
            <Keywords name="Delimiters">000000</Keywords>
            <Keywords name="Folder+">{</Keywords>
            <Keywords name="Folder-">}</Keywords>
            <Keywords name="Operators">( ) , . ; &lt; &gt;</Keywords>
            <Keywords name="Comment"> 1 2 2 0//</Keywords>
            <Keywords name="Words1">
				default do else for if jump return state while float integer key list quaternion rotation string vector
			</Keywords>
            <Keywords name="Words2">
START:functions
				%name%
END:functions
			</Keywords>
            <Keywords name="Words3">
START:constants
				%name%
END:constants
			</Keywords>
            <Keywords name="Words4">
START:events
				%name%
END:events
			</Keywords>
        </KeywordLists>
        <Styles>
            <WordsStyle name="DEFAULT" styleID="11" fgColor="000000" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="FOLDEROPEN" styleID="12" fgColor="400080" bgColor="FFFFFF" fontName="" fontStyle="1" fontSize="10" />
            <WordsStyle name="FOLDERCLOSE" styleID="13" fgColor="400080" bgColor="FFFFFF" fontName="" fontStyle="1" fontSize="10" />
            <WordsStyle name="KEYWORD1" styleID="5" fgColor="008040" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="KEYWORD2" styleID="6" fgColor="A65B42" bgColor="FFFFFF" fontName="" fontStyle="1" />
            <WordsStyle name="KEYWORD3" styleID="7" fgColor="0000A0" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="KEYWORD4" styleID="8" fgColor="0080C0" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="COMMENT" styleID="1" fgColor="000000" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="COMMENT LINE" styleID="2" fgColor="FF8000" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="NUMBER" styleID="4" fgColor="FF0080" bgColor="FFFFFF" fontName="" fontStyle="1" />
            <WordsStyle name="OPERATOR" styleID="10" fgColor="0000A0" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="DELIMINER1" styleID="14" fgColor="000000" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="DELIMINER2" styleID="15" fgColor="000000" bgColor="FFFFFF" fontName="" fontStyle="0" />
